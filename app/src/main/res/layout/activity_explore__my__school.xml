<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context=".Explore_My_School"
    android:id="@+id/">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Explore My Schools"
        android:id="@+id/textView"
        android:layout_marginTop="65dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/ovulogo.PNG"
        android:contentDescription="OVUlogo.PNG"
        android:layout_below="@+id/textView"
        android:layout_alignStart="@+id/textView"
        android:layout_alignEnd="@+id/textView"
        android:src="@drawable/ovulogo" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/Stark"
        android:src="@drawable/starkstate_logo"
        android:layout_below="@+id/OVUlogo.PNG"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="64dp" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/OVU"
        android:layout_above="@+id/imageView"
        android:layout_alignEnd="@+id/OVUlogo.PNG"
        android:layout_alignTop="@+id/OVUlogo.PNG"
        android:layout_alignStart="@+id/OVUlogo.PNG" />

</RelativeLayout>
